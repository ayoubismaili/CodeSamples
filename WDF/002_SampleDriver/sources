TARGETNAME=SampleDriver
TARGETTYPE=DRIVER
KMDF_VERSION=1

MSC_WARNING_LEVEL=/WX /W4

INCLUDES=..;.;

C_DEFINES=$(C_DEFINES) -DNDIS_WDM=1

C_DEFINES=$(C_DEFINES) -DNDIS60=1

C_DEFINES=$(C_DEFINES) -DNDISLWF=1
C_DEFINES=$(C_DEFINES) -DEVENT_TRACING=0

#pragma warning( disable: 4201 ) // nonstandard extension used : nameless struct/union
#pragma warning( disable: 4214 ) // nonstandard extension used : bit field types other than int
MSC_WARNING_LEVEL=$(MSC_WARNING_LEVEL) /wd4201 /wd4214

#PRECOMPILED_INCLUDE=precomp.h


TARGETLIBS=$(DDK_LIB_PATH)\ndis.lib

SOURCES=\
    SampleDriver.c
    

RUN_WPP= \
    $(SOURCES) \
    -km \
    -func:DEBUGP(LEVEL,MSG,...)\
	-scan:trace.h

